{
    "name": "tightenco/jazz",
    "description": "JavaScript-inspired conveniences for PHP programmers.",
    "keywords": [
        "tightenco",
        "jazz",
        "php"
    ],
    "homepage": "https://github.com/tighten/jazz",
    "license": "MIT",
    "authors": [
        {
            "name": "Matt Stauffer",
            "email": "matt@tighten.co",
            "homepage": "https://tighten.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/collections": "^9.0",
        "phplucidframe/console-table": "^1.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0",
        "tightenco/duster": "^0.4"
    },
    "autoload": {
        "psr-4": {
            "Tighten\\Jazz\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tighten\\Jazz\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
